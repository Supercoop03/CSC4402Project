<h1>Edit Worker</h1>
<form action="/workers/edit/<%= worker.WorkerID %>" method="POST">
  <label for="name">Name:</label>
  <input type="text" id="name" name="name" value="<%= worker.Name %>" required><br>
  
  <label for="address">Address:</label>
  <input type="text" id="address" name="address" value="<%= worker.Address %>" required><br>
  
  <label for="contactInfo">Contact Info:</label>
  <input type="text" id="contactInfo" name="contactInfo" value="<%= worker.ContactInfo %>" required><br>
  
  <label for="skills">Skills:</label>
  <input type="text" id="skills" name="skills" value="<%= worker.Skills %>" required><br>

  <label for="availability">Availability:</label>
  <input type="text" id="availability" name="availability" value="<%= worker.Availability %>" required><br>
  
  <label for="assignmentId">Assignment ID:</label>
  <input type="number" id="assignmentId" name="assignmentId" value="<%= worker.AssignmentID %>"><br>
  
  <button type="submit">Update Worker</button>
</form>
